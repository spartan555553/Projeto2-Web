<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<persistence xmlns="https://jakarta.ee/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="default">
    <class>entity.ClienteEntity</class>
    <class>entity.EnchidoEntity</class>
    <class>entity.EntregaEntity</class>
    <class>entity.FornecedorEntity</class>
    <class>entity.FuncionarioEntity</class>
    <class>entity.LoteEntity</class>
    <class>entity.MateriaPrimaEntity</class>
    <class>entity.PedidoClienteEntity</class>
    <class>entity.PedidoItemEntity</class>
    <class>entity.TransportadoraEntity</class>
    <class>entity.ErrorReportEntity</class>
    <properties>
      <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:55000/proj2"/>
      <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
      <property name="hibernate.connection.username" value="postgres"/>
      <property name="hibernate.connection.password" value="postgrespw"/>
    </properties>
  </persistence-unit>
</persistence>
